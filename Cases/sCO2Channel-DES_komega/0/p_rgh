FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform  7.633E6; 

boundaryField
{
    BotWall1
    {
	    	type			zeroGradient;
    }
    BotWall2
    {
	    	type			zeroGradient;
    }
    OtherWalls
    {
	    	type			zeroGradient;
    }

    Inlet
    {
        type               fixedValue;
        value              uniform 7.63883E6;
    }
    Outlet
    {
        type               fixedValue;
        value              uniform 7.62883E6;
    }
}

