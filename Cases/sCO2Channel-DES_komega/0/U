FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.8326); //(0 0 0);

boundaryField
{
    BotWall1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    BotWall2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    OtherWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    Inlet
    {
        type            pressureInletUniformVelocity;
        value           uniform (0 0 1);
	}

    Outlet
    {
        value           uniform (0 0 0.8326);
        type            fluxCorrectedVelocity;
    }
}

// ************************************************************************* //
