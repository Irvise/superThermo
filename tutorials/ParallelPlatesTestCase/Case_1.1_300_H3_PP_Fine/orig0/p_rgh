FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 8.1086299E6;

boundaryField
{
    "(BottWall|TopWall)"
    {
        type            fixedFluxPressure;
        value           uniform 8.1086299E6;
    }
    SideWall1
    {
        type            cyclic;
    }
    SideWall2
    {
        type            cyclic;
    }
    Inlet
    {
        type            cyclic;
    }
    Outlet
    {
        type            cyclic;
    }
}


