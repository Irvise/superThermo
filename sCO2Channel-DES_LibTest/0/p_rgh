{
    BotWall
    {
        type            fixedFluxPressure;
        value           uniform 0;
        adjoint         false;
    }
    OtherWalls
    {
        type            fixedFluxPressure;
        value           uniform 0;
        adjoint         false;
    }

    Inlet
    {
	  	type			zeroGradient;
       // type            fixedFluxPressure;
   	  //  value           uniform 0;
	//	type            fixedValue;  
	//	value           uniform 7.7719E6;
    }
    Outlet
    {
	//	type			zeroGradient;
		type            fixedValue;  
		value           uniform  7.769E6; //0; 2.5708554E7;
    /*    type            totalPressure;
		gamma			1.0;
        p0              uniform 7.769E6; */
    }
}

